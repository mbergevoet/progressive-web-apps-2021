<% characters.map(character=> { %>
    <article>
        <h2>
            <%= character.name %>
        </h2>
        <!-- <img srcset="/images/character-icon-320w.png 320w, /images/character-icon-480w.png 480w, /images/character-icon-640w.png 640w"
            sizes="(max-width: 480px) 320px, (max-width: 768px) 480px, (max-width: 1024px) 640px"
            src="/images/character-icon-640w.png" alt="placeholder character icon"> -->
        <img src="/images/character-icon.svg" alt="placeholder character icon">
        <div>
            <p>
                Height:
                <span>
                    -=<%= character.height %>=-
                </span>
            </p>
            <p>
                Gender:
                <span>
                    -=<%= character.gender %>=-
                </span>
            </p>
            <p>
                Eye Color:
                <span>
                    -=<%= character.eye_color %>=-
                </span>
            </p>
            <p>
                Hair Color:
                <span>
                    -=<%= character.hair_color %>=-
                </span>
            </p>
            <p>
                Year of Birth:
                <span>
                    -=<%= character.birth_year %>=-
                </span>
            </p>
            <p>
                Weight:
                <span>
                    -=<%= character.mass %>=-
                </span>
            </p>
        </div>
    </article>
    <% }) %>